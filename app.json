{
  "expo": {
    "name": "asteroid_native",
    "slug": "asteroid_native",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "description": "Get your AstroScope today.",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/meteor_icon.png",
    "splash": {
      "image": "./assets/astrosplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stevenreubenstone.astroscope"
    },
    "android": {
      "package": "com.stevenreubenstone.astroscope"
    }
  }
}